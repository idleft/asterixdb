drop  dataverse feeds if exists;
create  dataverse feeds;
use feeds;

create type TweetType as open {
  id : int64
};

create type CustomerType as open {
  id : int64
};

create type emptyType as open {
};

create dataset Customer(CustomerType) primary key id;
create dataset Tweets1(TweetType) primary key id;

create feed TweetFeed with {
  "adapter-name" : "expr_twitter",
  "ingestion-location":"asterix_nc1",
  "type-name":"TweetType",
  "format" : "adm",
  "expr_amount" : 100
};

// join on primary key
create function func10(x) {
  object_merge(x, {"q" : (select value c from Customer c where c.country = x.country)})
};
